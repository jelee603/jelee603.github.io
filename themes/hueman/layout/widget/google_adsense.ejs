<div class="widget-wrap widget-float">
  <% if (theme.plugins.google_adsense ) { %>
  <!-- huemen sidebar -->
  <script
    async
    src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-7250093750072260"
    crossorigin="anonymous"
  ></script>
  <!-- 블로그 사이드광고 -->
  <ins
    class="adsbygoogle"
    style="display: block"
    data-ad-client="ca-pub-7250093750072260"
    data-ad-slot="4932064801"
    data-ad-format="auto"
    data-full-width-responsive="true"
  ></ins>
  <script>
    (adsbygoogle = window.adsbygoogle || []).push({});
  </script>
  <% } else { %>
  <h3 class="widget-title"><%= __('adsense.sponsored') %></h3>
  <div class="widget">
    <a href="https://marcofranssen.nl?adsense_widget"
      >This widget is sponsored by, Marco Franssen, main contributor to this
      theme.</a
    >
  </div>
  <% } %>
</div>
