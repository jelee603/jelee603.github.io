<% if (post.contents && post.contents.length) { %>
    <div class="widget-wrap">
        <h3 class="widget-title">Contents</h3>
        <div class="widget">
            <ul id="contents-list">
                <% post.contents.forEach(function(post, index) {
                    const [text, url] = post.split('|');    
                %>
                    <li>
                        <div class="contents-inner">
                            <p class="item-title">
                                <%= index + 1 %>. <a href="<%=url%>"><%= text %></a>
                            </p>
                        </div>
                    </li>
                <% }) %>
            </ul>
        </div>
    </div>
    <div class="contents-bottom"></div>
<% } %>